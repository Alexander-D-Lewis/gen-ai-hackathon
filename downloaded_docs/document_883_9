    <article id="post-227" class="post-227 post type-post status-publish format-standard hentry category-heimwerken">
    
    	<div class="meta-img">
		        	<div class="entry-nothumb">
            	                	<div class="noimg-bg"></div>
                    <div class="dateonimg">
                        <span class="pdate">14</span><span class="pmonth">Sep/22</span>
                    </div>
